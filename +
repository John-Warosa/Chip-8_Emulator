#include "ram.h"
#include "defines.h"
#include <stdio.h>
#include <stdlib.h>


int main() {
    u8 *const ram = RAM_init();

    for (int i = 0x50; i < 0xa0; i += 5) {
        printf("%x: %x\n", i, ram[i]);
        printf("%x: %x\n", i+1, ram[i+1]);
        printf("%x: %x\n", i+2, ram[i+2]);
        printf("%x: %x\n", i+3, ram[i+3]);
        printf("%x: %x\n", i+4, ram[i+4]);
        putchar('\n');
    }

    free(ram);
}
